    "@babel/preset-typescript": "^7.10.4",
    "@types/async": "^3.2.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^8.2.2",
    "@types/node": "^16.11.19",
    "@types/relateurl": "^0.2.28",
    "@types/xmldoc": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
