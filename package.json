      "name": "Grzegorz Godlewski",
      "url": "https://github.com/ggodlewski"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mieweb/wikiGDrive/issues"
  },
  "homepage": "https://github.com/mieweb/wikiGDrive#readme",
  "dependencies": {
    "async": "^3.2.0",
    "buffer": "^5.7.1",
    "cli-progress": "^3.9.1",
    "dotenv": "^8.6.0",
    "enquirer": "^2.3.6",
    "express": "^4.17.1",
    "googleapis": "^71.0.0",
    "identify-filetype": "^1.0.0",
    "jszip": "^3.7.0",
    "listr2": "^3.14.0",
    "minimist": "^1.2.5",
    "node-fetch": "^2.6.6",
    "nunjucks": "^3.2.3",
    "open": "^7.4.2",
    "relateurl": "^0.2.7",
    "sharp": "^0.29.3",
    "sharp-phash": "^2.0.0",
    "simple-git": "^3.3.0",
    "slugify": "^1.6.5",
    "stream": "^0.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
