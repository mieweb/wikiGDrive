  "lint": {
    "include": ["src/", "test/"],
    "exclude": ["website/public/", "website/.vitepress/", "apps/ui"],
    "rules": {
      "tags": ["recommended"],
      "exclude": [
        "no-explicit-any",
        "prefer-const",
        "no-unused-vars",
        "no-async-promise-executor",
        "require-await"
      ]
    }
  },
  "fmt": {
    "singleQuote": true
  },
  "tasks": {
  },
  "test": {
    "include": [
      "test/*Test.ts",
      "test/**/*.ts"
    ]
